ent.getElementById(\"feedback-form\");\n    elem.setAttribute('aria-hidden', !open);\n    if (open && !isFormLoaded) {\n      const elem = document.getElementById(\"feedback-form-container\");\n      elem.innerHTML = `<iframe src=\"https://www.cognitoforms.com/f/${formDataKey}/${formDataId}\" width=\"100%\" height=\"532px\"></iframe>`;\n      isFormLoaded = true;\n    }\n  }\n</script>\n\n    <div id=\"overlay\" class=\"hidden\" onclick=\"toggleMenu('menu', false)\"></div>\n</div>\n\n\n<script\n    src=\"https://code.jquery.com/jquery-3.5.1.min.js\"\n    integrity=\"sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=\"\n    crossorigin=\"anonymous\">\n</script>\n<script src=\"https://polyfill.io/v3/polyfill.min.js?features=Promise%2CObject.entries%2CObject.assign%2CURLSearchParams%2CURL%2CclassList\"></script>\n<script\n  src=\"https://cdn.jsdelivr.net/npm/algoliasearch@4.14.3/dist/algoliasearch-lite.umd.js\"\n  integrity=\"sha256-dyJcbGuYfdzNfifkHxYVd/rzeR6SLLcDFYEidcybldM=\"\n  crossorigin=\"anonymous\"\n></script>\n<script src=\"https://cdn.jsdelivr.net/npm/@algolia/autocomplete-js\"></script>\n<script src=\"https://www.tayohelp.com/assets/built/tayohelp-ghost-theme.js?v=297e07b37b\"></script>\n<script>\n$(document).ready(function () {\n    // Mobile Menu Trigger\n    $('.gh-burger').click(function () {\n        $('body').toggleClass('gh-head-open');\n    });\n    // FitVids - Makes video embeds responsive\n    $(\".article\").fitVids();\n    // Clean Tag Names\n    cleanTagNames();\n});\n</script>\n<script>\nconst { autocomplete, getAlgoliaResults } = window['@algolia/autocomplete-js'];\nfunction debouncePromise(fn, time) {\n  let timerId = undefined;\n\n  return function debounced(...args) {\n    if (timerId) {\n      clearTimeout(timerId);\n    }\n\n    return new Promise((resolve) => {\n      timerId = setTimeout(() => resolve(fn(...args)), time);\n    });\n  };\n}\nconst debounced = debouncePromise((items) => Promise.resolve(items), 500);\n\nfunction setupAutocomplete () {\n  const algoliaAppId = \"987PCKS7BK\"\n  const algoliaSearchApiKey = \"95c6d6de50f4b27d6c83aeb55194273d\"\n  const algoliaSearchIndex = \"posts\"\n  const localeFilter = \"NOT tags.slug:hash-locale-tl-ph\"\n\n  const searchClient = algoliasearch(algoliaAppId, algoliaSearchApiKey);\n  autocomplete({\n    container: '#autocomplete',\n    panelContainer: \"#autocomplete\",\n    placeholder: \"Search for a topic or question\",\n    insights: true,\n    onSubmit(event) {\n      window.location.href = `/en-us/search/?query=${encodeURIComponent(event.state.query)}`;\n    },\n    getSources({ query }) {\n      return debounced([\n        {\n          sourceId: \"algolia\",\n          insights: false,\n          getItems() {\n            return getAlgoliaResults({\n              searchClient,\n              queries: [\n                {\n                  indexName: algoliaSearchIndex,\n                  query,\n                  filters: `(NOT tags.slug:hash-list-page) AND (${localeFilter})`,\n                  params: {\n                    hitsPerPage: 5,\n                    attributesToSnippet: ['title:20'],\n                  },\n                },\n              ],\n            });\n          },\n          templates: {\n            item({ item, components, html }) {\n              return html`<a href=\"${item.url}\" class=\"aa-ItemWrapper\">\n                <div class=\"aa-ItemContent\">\n                  <div class=\"aa-ItemContentBody\">\n                    <div class=\"aa-ItemContentTitle\">\n                      ${components.Highlight({\n                        hit: item,\n                        attribute: 'title',\n                      })}\n                    </div>\n                  </div>\n                </div>\n              </a>`;\n            },\n          },\n        },\n      ]);\n    },\n  });\n}\ndocument.addEventListener(\"DOMContentLoaded\", setupAutocomplete);\n</script>\n\n\n\n<script>\n$(function() {\n$('.gh-content a').filter(function() {\n\treturn this.hostname && this.hostname !== location.hostname;\n}).attr('target', '_blank');\n});\n</script>\n\n</body>\n</html>\n"}