    "acf": {
      "ultima_hora": false,
      "titulo_home": "No son grupos armados disparándole a un avión de la fuerza aérea en Sinaloa",
      "imagen_video_destacado": {
        "tipo_de_contenido": "Imagen",
        "imagen_destacada": {
          "imagen": "https://admin.animalpolitico.com/wp-content/uploads/2023/01/Plantilla-Frasua-11-300x149-1.png",
