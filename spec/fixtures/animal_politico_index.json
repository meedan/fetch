    "acf": {
      "ultima_hora": false,
      "titulo_home": "Video de disparos desde una aeronave no es de operativo por captura de Ovidio",
      "imagen_video_destacado": {
        "tipo_de_contenido": "Imagen",
        "imagen_destacada": {
          "imagen": "https://admin.animalpolitico.com/wp-content/uploads/2023/01/sacado-contexto-disparos-michoacan-ovidio-300x160-1.png",
          "pie_imagen": ""
