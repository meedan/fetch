    "acf": {
      "ultima_hora": false,
      "titulo_home": "¿Comando armado tomó el puerto de Culiacán? No, video se grabó en Venecia",
      "imagen_video_destacado": {
        "tipo_de_contenido": "Imagen",
        "imagen_destacada": {
          "imagen": "https://admin.animalpolitico.com/wp-content/uploads/2023/01/puerto-culiacan-falso.png",
