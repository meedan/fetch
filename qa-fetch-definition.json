{
    "containerDefinitions": [
        {
            "name": "qa-fetch",
            "image": "848416313321.dkr.ecr.eu-west-1.amazonaws.com/fetch:14c9f55bf6dad5b8f381ab1a11c2881860c4cdb6",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 9292,
                    "hostPort": 9292,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "command": [
                "start_server"
            ],
            "environment": [
                {
                    "name": "APP",
                    "value": "fetch"
                },
                {
                    "name": "DEPLOY_ENV",
                    "value": "qa"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "GITHUB_TOKEN",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-1:848416313321:secret:GithubToken-Plain-BUhwIw"
                },
                {
                    "name": "redis_host",
                    "valueFrom": "/QA/fetch/redis_host"
                },
                {
                    "name": "redis_database",
                    "valueFrom": "/QA/fetch/redis_database"
                },
                {
                    "name": "es_host",
                    "valueFrom": "/QA/fetch/es_host"
                },
                {
                    "name": "es_index_name",
                    "valueFrom": "/QA/fetch/es_index_name"
                },
                {
                    "name": "google_api_key",
                    "valueFrom": "/QA/fetch/google_api_key"
                },
                {
                    "name": "boom_live_api_key",
                    "valueFrom": "/QA/fetch/boom_live_api_key"
                },
                {
                    "name": "airbrake_api_host",
                    "valueFrom": "/QA/fetch/airbrake_api_host"
                },
                {
                    "name": "airbrake_api_key",
                    "valueFrom": "/QA/fetch/airbrake_api_key"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/qa-fetch",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "qa-fetch",
    "executionRoleArn": "arn:aws:iam::848416313321:role/ECSSecretsAccessRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "cpu": "1024",
    "memory": "1024"
}
